<body>

    <div id="outermost_container">
        <div id="signin_box">
            <div id="title">Sign-in</div>
                <form id="signin_form" method="post" action="/signin">
                    <div id="signin_user_id_div" class="form-group">
                        <label for="signin_user_id">ID</label>
                        <input id="signin_user_id" class="form-control" type="text" name="signin_user_id">
                    </div>
                    <div id="signin_user_password_div" class="form-group">
                        <label for="signin_user_password">PASSWORD</label>
                        <input id="signin_user_password" class="form-control" type="password" name="signin_user_password">
                    </div>
                    <div id="signin_fail_message" class="alert alert-danger">
                        Confirm your ID or PASSWORD
                    </div>
                        <button id="local_signin_button" class="btn btn-outline-secondary" type="submit">sign-in</button>
                        <button id="google_signin_button" >google sign-in</button>
                        <button id="facebook_signin_button" >facebook sign-in</button>
                        <button id="kakao_signin_button" >kakao sign-in</button>
                        <a id="find_account" href="/find_account">Find-account</a>
                        <a id="home" href="/">Home</a>
                </form>
        </div>
